{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Website</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Httpurl</th>
                <td>{{ entity.httpUrl }}</td>
            </tr>
            <tr>
                <th>Httpsurl</th>
                <td>{{ entity.httpsUrl }}</td>
            </tr>
            <tr>
                <th>Icpnum</th>
                <td>{{ entity.icpNum }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Zipcode</th>
                <td>{{ entity.zipCode }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ entity.tel }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ entity.fax }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('website') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('website_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
