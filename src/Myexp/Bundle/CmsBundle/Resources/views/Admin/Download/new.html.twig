{% extends 'MyexpCmsBundle:Admin:layout.html.twig' %}
{% block title %}{{ 'download.new'|trans }}{% endblock %}
{% block breadcrumd %}
        <a href="{{ path('admin') }}">{{ 'nav.home'|trans }}</a> ><a href="{{path('download')}}">{{ 'download.manage'|trans }}</a> ><a href="{{app.request.getRequestUri()}}">{{ 'download.new'|trans }}</a></span>
{% endblock %}
{% block content %}
<form action="{{ path('download_create') }}" method="post" {{ form_enctype(form) }}>
    <div class="form">
        {{ form_widget(form) }}        
        <div class="form-actions">
            <input type="submit" class="btn btn-primary" value="{{ 'common.submit'|trans }}" />
            <input type="reset" class="btn btn-danger" value="{{ 'common.reset'|trans }}" />
        </div>
    </div>
</form>
<form>
    <div class="form-actions">
        <input class="btn btn-primary" type="submit" value="{{ 'common.back_list'|trans }}"  onclick="saveToDraft(this, '{{ path('download') }}');" />
    </div>
</form>
{% endblock %}
