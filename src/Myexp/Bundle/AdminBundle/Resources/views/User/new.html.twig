{% extends 'MyexpAdminBundle::base.html.twig' %}
{% block title %}{{ 'user.new'|trans }} - {{ 'user.manage'|trans }}{{ parent() }}{% endblock %}
{% block breadcrumd %}
    <a class="section" href="{{ path('admin_user') }}">{{ 'user.manage'|trans }}</a>
    <i class="right angle icon divider"></i>
    <div class="active section">{{ 'user.new'|trans }}</div>
{% endblock %}
{% block content %}
    {{ form_start(form, {action: path('admin_user_create')}) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
{% endblock %}
